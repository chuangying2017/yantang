{
    "name": "rappasoft/laravel-5-boilerplate",
    "description": "The Laravel Framework Boilerplate Project.",
    "keywords": [
        "framework",
        "laravel",
        "boilerplate"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "5.1.*",
        "laravelcollective/html": "5.1.*",
        "laravel/socialite": "~2.0",
        "twbs/bootstrap": "3.*",
        "twbs/bootstrap-sass": "3.*",
        "fortawesome/font-awesome": "4.*",
        "hieu-le/active": "~2.0",
        "doctrine/dbal": "~2.5",
        "laracasts/utilities": "~2.1",
        "arcanedev/log-viewer": "~3.5.0",
        "creativeorange/gravatar": "~1.0",
        "bugsnag/bugsnag-laravel": "1.*",
        "maatwebsite/excel": "~2.0.0",
        "pda/pheanstalk": "~3.0",
        "predis/predis": "~1.0",
        "pingplusplus/pingpp-php": "dev-master",
        "zgldh/qiniu-laravel-storage": "^0.3.0",
        "baum/baum": "~1.1",
        "dingo/api": "1.0.x@dev",
        "tymon/jwt-auth": "0.5.*",
        "barryvdh/laravel-cors": "^0.7.1",
        "toplan/phpsms": "~1.2.0",
        "toplan/laravel-sms": "~2.2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "laracasts/generators": "~1.1",
        "potsky/laravel-localization-helpers": "~1.1",
        "barryvdh/laravel-debugbar": "^2.0",
        "doctrine/dbal": "~2.3",
        "laracasts/testdummy": "^2.3",
        "barryvdh/laravel-ide-helper": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
